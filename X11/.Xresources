xterm*termName:     xterm-color

*background: #181616
*foreground: #c5c9c5
*cursor: #C8C093

! normal
*.color0: #0d0c0c
*.color1: #c4746e
*.color2: #8a9a7b
*.color3: #c4b28a
*.color4: #8ba4b0
*.color5: #a292a3
*.color6: #8ea4a2
*.color7: #C8C093

! bright
*.color8:  #a6a69c
*.color9:  #E46876
*.color10: #87a987
*.color11: #E6C384
*.color12: #7FB4CA
*.color13: #938AA9
*.color14: #7AA89F
*.color15: #c5c9c5

xft.dpi:            96
xterm*font:	    	xft:UbuntuMono Nerd Font Mono:Style=Normal:size=14:antialias=true:
xterm*renderfont:   true

xterm*letterSpace: 	0
xterm*lineSpace:    0
xterm*cursorBlink:  false
xterm*borderLess: 	true
xterm*borderWidth:  0
xterm*externalBorder: 	0
xterm*internalBorder: 	12
xterm*scrollBar: 	false
xterm*saveLines:	5000
xterm*depth:	    24

*VT100*Translations: #override \n\
    Ctrl <Key>C: copy-selection(CLIPBOARD) \n\
    Ctrl <Key>V: insert-selection(CLIPBOARD)
